# Blockchain Service Environment Configuration
# Copy this file to .env and fill in your actual values

# Service Configuration
PORT=4004
NODE_ENV=development

# Blockchain Configuration
GANACHE_URL=http://127.0.0.1:8545
BLOCKCHAIN_NETWORK_ID=1337

# Smart Contract Addresses (will be auto-updated after deployment)
NFT_CONTRACT_ADDRESS=YOUR_NFT_CONTRACT_ADDRESS_HERE
MARKETPLACE_CONTRACT_ADDRESS=YOUR_MARKETPLACE_CONTRACT_ADDRESS_HERE

# Admin Private Key (CHANGE THIS TO YOUR ACTUAL PRIVATE KEY)
ADMIN_PRIVATE_KEY=YOUR_ADMIN_PRIVATE_KEY_HERE

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/viepropchain

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:4000
